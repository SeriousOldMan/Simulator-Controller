;;;-------------------------------------------------------------------------;;;
;;;                            Initialization                               ;;;
;;;-------------------------------------------------------------------------;;;

[?Initialize] =>    (Prove: removeSettings("Assetto Corsa", "Porsche 911 GT3 Cup 2017",
										   [Aero.Wing.Front, Electronics.TC,
											Bumpstop.Range.Front.Left, Bumpstop.Range.Rear.Left, Bumpstop.Range.Front.Right, Bumpstop.Range.Rear.Right,
											Bumpstop.Rate.Front.Left, Bumpstop.Rate.Rear.Left, Bumpstop.Rate.Front.Right, Bumpstop.Rate.Rear.Right,
											Damper.Compression.Slow.Front.Left, Damper.Compression.Slow.Rear.Left,
											Damper.Compression.Slow.Front.Right, Damper.Compression.Slow.Rear.Right,
											Damper.Compression.Fast.Front.Left, Damper.Compression.Fast.Rear.Left,
											Damper.Compression.Fast.Front.Right, Damper.Compression.Fast.Rear.Right,
											Damper.Rebound.Fast.Front.Left, Damper.Rebound.Fast.Rear.Left,
											Damper.Rebound.Fast.Front.Right, Damper.Rebound.Fast.Rear.Right,
											Damper.Rebound.Slow.Front.Left, Damper.Rebound.Slow.Rear.Left,
											Damper.Rebound.Slow.Front.Right, Damper.Rebound.Slow.Rear.Right,
											Differential.Coast, Differential.Power, Differential.Preload,
											Spring.Rate.Front.Left, Spring.Rate.Rear.Left, Spring.Rate.Front.Right, Spring.Rate.Rear.Right])),
					(Prove: addSettings("Assetto Corsa", "Porsche 911 GT3 Cup 2017",
										[Aero.Height.Front.Left, Aero.Height.Front.Right, Aero.Height.Rear.Left, Aero.Height.Rear.Right]))
;;;-------------------------------------------------------------------------;;;
;;;                            Initialization                               ;;;
;;;-------------------------------------------------------------------------;;;

[?Initialize] => (Prove: addSettings("Le Mans Ultimate", "Ginetta G61-LT-P325 Evo",
									 [Spring.Rate.Front.Heave, Spring.Rate.Rear.Heave,
									  Bumpstop.Range.Front.Heave, Bumpstop.Range.Rear.Heave,
									  Differential.Coast, Differential.Power])),
				 (Prove: removeSettings("Le Mans Ultimate", "Ginetta G61-LT-P325 Evo", [Electronics.Map, Aero.Splitter.Front]))
;;;-------------------------------------------------------------------------;;;

[?Initialize] => (Prove: addSettings("Le Mans Ultimate", "Oreca 07 ELMS",
									 [Bumpstop.Range.Front.Heave, Bumpstop.Range.Rear.Heave,
									  Differential.Coast, Differential.Power])),
				 (Prove: removeSettings("Le Mans Ultimate", "Oreca 07 ELMS", [Aero.Splitter.Front]))